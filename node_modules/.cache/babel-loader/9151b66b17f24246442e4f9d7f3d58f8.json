blic_key }} </p>\n        <p>{{ private_key }} </p>\n    </b-row>\n\n    <b-row>\n      <b-col cols=12  >\n        <b-form @submit.prevent=\"onSent\">\n          <b-form-textarea\n            v-model=\"text\"\n            placeholder=\"Enter something...\"\n            rows=\"3\"\n            max-rows=\"6\"\n            required\n            \n          ></b-form-textarea>\n\n          <pre class=\"mt-3 mb-0\">{{ text }}</pre>\n\n          <div class=\"d-flex justify-content-end \">\n            <b-button type=\"submit\" variant=\"primary\"> Bấm gửi </b-button>\n          </div>\n          \n      </b-form>\n      </b-col>\n  \n    </b-row>\n\n    <b-row class=\"mb-4\">\n      <b-col cols=12 class=\"h2\">\n        Chữ được mã hóa\n      </b-col>\n      <b-col style =\"height : 100px;\" cols=12 class =\"border border-primary\">\n        {{ encrypted_text }}\n      </b-col>\n    </b-row>\n\n    <b-row>\n      <b-col cols=12 class=\"h2\">\n        Chữ được giải mã\n      </b-col>\n      <b-col style =\"height : 100px;\" cols=12 class =\"border border-primary\">\n        {{ decrypted_text }}\n      </b-col>\n    </b-row>\n  \n  </b-container>\n</template>\n\n<script>\nimport NodeRSA from \"node-rsa\"\n// import TextDecoder from \"text-encoding\"\nexport default {\n  data(){\n    return {\n      text : \"\",\n      public_key : \"\",\n      private_key : \"\",\n      key : \"\",\n      encrypted_text : \"\",\n      decrypted_text : \"\"\n    }\n  },\n  methods : {\n    create_key : function(){\n      \n      this.key = new NodeRSA({b: 512});\n      this.public_key = this.key.exportKey('public');\n      this.private_key = this.key.exportKey('components-public');\n    },\n    onSent  : function(){\n      this.encrypted_text = this.key.encrypt(this.text, 'base64');\n      this.decrypted_text = this.key.decrypt(this.encrypted_text, 'utf8');\n\n    }\n  },\n  computed : {\n  \n  },\n  created(){\n    this.create_key()\n  }\n}\n</script>\n\n<style>\n\n</style>"],"sourceRoot":"