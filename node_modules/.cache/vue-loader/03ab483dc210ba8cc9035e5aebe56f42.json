l cols=12 class=\"h2\">\n        Chữ được mã hóa\n      </b-col>\n      <b-col style =\"height : 100px;\" cols=12 class =\"border border-primary\">\n        {{ encrypted_text }}\n      </b-col>\n    </b-row>\n\n    <b-row>\n      <b-col cols=12 class=\"h2\">\n        Chữ được giải mã\n      </b-col>\n      <b-col style =\"height : 100px;\" cols=12 class =\"border border-primary\">\n        {{ decrypted_text }}\n      </b-col>\n    </b-row>\n  \n  </b-container>\n</template>\n\n<script>\nimport NodeRSA from \"node-rsa\"\n// import TextDecoder from \"text-encoding\"\nexport default {\n  data(){\n    return {\n      text : \"\",\n      public_key : \"\",\n      private_key : \"\",\n      key : \"\",\n      encrypted_text : \"\",\n      decrypted_text : \"\"\n    }\n  },\n  methods : {\n    create_key : function(){\n      \n      this.key = new NodeRSA({b: 512});\n      this.public_key = this.key.exportKey('public');\n      this.private_key = this.key.exportKey('components-public');\n    },\n    onSent  : function(){\n      this.encrypted_text = this.key.encrypt(this.text, 'base64');\n      this.decrypted_text = this.key.decrypt(this.encrypted_text, 'utf8');\n\n    }\n  },\n  computed : {\n  \n  },\n  created(){\n    this.create_key()\n  }\n}\n</script>\n\n<style>\n\n</style>"]}]}